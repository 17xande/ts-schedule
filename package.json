{
  "name": "ts-schedule",
  "version": "0.6.2",
  "description": "Schedule showing and hiding of DOM elements.",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "rollup -c",
    "build": "npm run build:types && npm run build:js",
    "watch:js": "rollup -c -w",
    "watch": "npm run build:types && npm run watch:js"
  },
  "main": "src/index.ts",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/17xande/ts-schedule.git"
  },
  "keywords": [
    "schedule",
    "dom",
    "typescript"
  ],
  "author": "Alexandre Figueiredo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/17xande/ts-schedule/issues"
  },
  "homepage": "https://github.com/17xande/ts-schedule#readme",
  "dependencies": {
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.33"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-typescript": "^7.15.0",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "rollup": "^2.56.3",
    "typescript": "^4.3.5"
  }
}
