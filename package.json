{
  "name": "ts-schedule",
  "version": "0.4.1",
  "description": "Schedule showing and hiding of DOM elements.",
  "main": "index.ts",
  "directories": {
    "example": "example"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run build:types && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir lib --extensions \".ts,.tsx\" --source-maps inline",
    "bundle:dev": "webpack --mode=development",
    "bundle": "webpack"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/17xande/ts-schedule.git"
  },
  "keywords": [
    "schedule",
    "dom",
    "typescript"
  ],
  "author": "Alexandre Figueiredo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/17xande/ts-schedule/issues"
  },
  "homepage": "https://github.com/17xande/ts-schedule#readme",
  "dependencies": {
    "moment": "*"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.10.5",
    "@babel/plugin-proposal-class-properties": "*",
    "@babel/preset-env": "*",
    "@babel/preset-typescript": "*",
    "babel-loader": "*",
    "typescript": "^3.9.7"
  }
}
